<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<?import javafx.geometry.Insets?>
<AnchorPane prefHeight="208.0" prefWidth="574.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.tama.trial.controller.MainController">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
   </padding>
   <children>
      <Label fx:id="lblInput" layoutX="20.0" layoutY="33.0" text="Input" AnchorPane.leftAnchor="20.0"  AnchorPane.topAnchor="33.0" />
      <TextField fx:id="txtInput" layoutX="80.0" layoutY="29.0" AnchorPane.leftAnchor="80.0" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="29.0" />
      <Button fx:id="btnSend" layoutX="80.0" layoutY="75.0" mnemonicParsing="false" onAction="#sendText" prefHeight="30.0" prefWidth="120.0" text="Send" AnchorPane.leftAnchor="80.0" AnchorPane.topAnchor="75.0" />
      <Label fx:id="lblOutput" layoutX="80.0" layoutY="120.0" AnchorPane.leftAnchor="80.0" AnchorPane.topAnchor="120.0" />
   </children>
</AnchorPane>
